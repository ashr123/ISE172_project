<Page x:Class="WPF_App.HistoryPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WPF_App"
      mc:Ignorable="d" 
      Height="350" Width="525" Background="AliceBlue" 
      Title="HistoryPage">
	<DataGrid x:Name="HistoryDataGrid" HorizontalAlignment="Left" Height="330" Margin="10,10,0,0" VerticalAlignment="Top" Width="505" ItemsSource="{Binding History}" CanUserAddRows="False"/>

</Page>
